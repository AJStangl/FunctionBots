{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "submissionTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 0/1 * * *",
      "monitor": true,
      "runOnStartUp": true
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "responseMessage",
      "queueName": "submission-worker",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
