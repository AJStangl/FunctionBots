{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "genTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "*/3 * * * * *",
      "useMonitor": true,
      "runOnStartup": true
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "responseMessage",
      "queueName": "reply-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
