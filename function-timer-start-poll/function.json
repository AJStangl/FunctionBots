{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "initializingTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0/4 * * * *",
      "useMonitor": true
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "poll-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
