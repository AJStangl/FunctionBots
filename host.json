{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "extensions": {
    "queues": {
      "batchSize": 2,
      "newBatchThreshold": 1
    },
    "tables": {}
  },
  "logging": {
    "fileLoggingMode": "never",
    "logLevel": {
      "default": "Information",
      "Host": "None",
      "Function": "Information",
      "Host.Aggregator": "None",
      "azure.data": "None",
      "azure.core.pipeline.policies.http_logging_policy": "Debug"
    }
  }
}