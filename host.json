{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:15",
      "visibilityTimeout" : "00:00:30",
      "maxDequeueCount": 5,
      "batchSize": 8,
      "newBatchThreshold": 0
    },
    "tables": {},
    "http": {
      "routePrefix": "api"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "azure.core.pipeline.policies.http_logging_policy": "None"
    }
  }
}