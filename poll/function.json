{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "contentTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */1 * * * *"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "content-queue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
